{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
        {
            "alignment": "left",
            "segments": [
                {
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": "<#CB4B16>\u250f[</>{{ .UserName }}@DEDSEC-DESKTOP<#CB4B16>]</><#CB4B16>-",
                    "type": "session"
                },
                {
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": "<#CB4B16>[</> \udb81\ude8c I'm root <#CB4B16>]</></>",
                    "type": "session"
                },
                {
                    "foreground": "#ffffff",
                    "style": "diamond",
                    "template": "<#CB4B16>-[</>{{ .Artist }} - {{ .Track }}<#CB4B16>]</>",
                    "type": "spotify"
                },
                {
                    "foreground": "#ffffff",
                    "style": "plain",
                    "template": "<#CB4B16>[</>{{ .HEAD }}<#CB4B16>]</>",
                    "type": "git"
                }
            ],
            "type": "prompt"
        },
        {
            "alignment": "left",
            "newline": true,
            "segments": [
                {
                    "foreground": "#ffffff",
                    "properties": {
                        "style": "mixed"
                    },
                    "style": "plain",
                    "template": "<#CB4B16>-[</>{{ .Path }}<#CB4B16>]></>",
                    "type": "path"
                }
            ],
            "type": "prompt"
        }
    ],
    "final_space": true,
    "version": 2
}