{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#CB4B16>\u250f[</>{{ .UserName }}@DEDSEC-DESKTOP<#CB4B16>]</>",
          "foreground": "#ffffff",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "{{ if .Root }}<#CB4B16>-[</> \udb81\ude8c I'm root <#CB4B16>]</></>{{end}}",
          "foreground": "#ffffff",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#CB4B16>-[</>{{ .Artist }} - {{ .Track }}<#CB4B16>]</>",
          "foreground": "#ffffff",
          "type": "spotify",
          "style": "diamond"
        },
        {
          "properties": {
            "cache_duration": "none"
          },
          "template": "<#CB4B16>[</>{{ .HEAD }}<#CB4B16>]</>",
          "foreground": "#ffffff",
          "type": "git",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "cache_duration": "none",
            "style": "mixed"
          },
          "template": "<#CB4B16>-[</>{{ .Path }}<#CB4B16>]></>",
          "foreground": "#ffffff",
          "type": "path",
          "style": "plain"
        }
      ],
      "newline": true
    }
  ],
  "version": 3,
  "final_space": true
}
